			If WinGetState($GLOBAL_MAIN_GUI) <> 47 Then
				If $aCurInfo[4] = 0 Then
					Local $Return_HT = 2, $Set_Cursor = 2

					Switch $CurSorInfo
						Case 1
							$Set_Cursor = 13
							$Return_HT = 10
						Case 2
							$Set_Cursor = 13
							$Return_HT = 11
						Case 3
							$Set_Cursor = 11
							$Return_HT = 12
						Case 4
							$Set_Cursor = 12
							$Return_HT = 13
						Case 5
							$Set_Cursor = 10
							$Return_HT = 14
						Case 6
